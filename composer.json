{
    "name": "codess/codess-github-issue-creator",
    "description": "A wordpress plugin to create github issues from codess media projects.",
    "type": "wordpress-plugin",
    "license": "Copyright",
    "autoload": {
        "psr-4": {
            "Codess\\CodessGitHubIssueCreator\\": "includes/"
        }
    },
    "authors": [
        {
            "name": "codess media GmbH",
            "email": "technik@codess.media"
        }
    ],
    "require": {
        "johnbillion/extended-cpts": "^5.0",
        "knplabs/github-api": "^3.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "http-interop/http-factory-guzzle": "^1.0"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
